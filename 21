#include<stdio.h>


int main(){
    int vetA[10];
    int vetB[10];
    int vetC[10];


    for(int i = 0; i < 10; i++){
        printf("Preecha o primeiro vetor: ");
        scanf("%d", &vetA[i]);
    }
    for(int i = 0; i < 10; i++){
        printf("Preecha o segundo vetor: ");
        scanf("%d", &vetB[i]);
    }
    for(int i = 0; i < 10; i++){
        vetC[i] = vetA[i] - vetB[i];
        printf("\nValores do vetor C: %d", vetC[i]);
    }
}
